<!-- See https://goo.gl/qRE0vM -->
<meta name="theme-color" content="#ffffff">

<!-- Add to homescreen for Chrome on Android. Fallback for manifest.json -->
<meta name="mobile-web-app-capable" content="yes">
<meta name="application-name" content="<%= config.title %>"/>

<!-- Default icons -->
<link rel="icon" href="/favicon.ico" type="image/x-icon">
<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
<link rel="icon" type="image/png" href="/icons/32.png" sizes="32x32">
<link rel="icon" type="image/png" href="/icons/192.png" sizes="192x192">
<link rel="icon" type="image/png" href="/icons/96.png" sizes="96x96">
<link rel="icon" type="image/png" href="/icons/16.png" sizes="16x16">
<link rel="mask-icon" href="/icons/icon.svg" color="#5bbad5">

<!-- Homescreen icons -->
<link rel="apple-touch-icon" href="/icons/48.png">
<link rel="apple-touch-icon" sizes="57x57" href="/icons/57.png">
<link rel="apple-touch-icon" sizes="60x60" href="/icons/60.png">
<link rel="apple-touch-icon" sizes="72x72" href="/icons/72.png">
<link rel="apple-touch-icon" sizes="76x76" href="/icons/76.png">
<link rel="apple-touch-icon" sizes="96x96" href="/icons/96.png">
<link rel="apple-touch-icon" sizes="120x120" href="/icons/120.png">
<link rel="apple-touch-icon" sizes="144x144" href="/icons/144.png">
<link rel="apple-touch-icon" sizes="152x152" href="/icons/152.png">
<link rel="apple-touch-icon" sizes="180x180" href="/icons/180.png">
<link rel="apple-touch-icon" sizes="192x192" href="/icons/192.png">
<!-- Add to homescreen for Safari on iOS -->
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
<meta name="apple-mobile-web-app-title" content="<%= config.title %>">
<!-- specifying a startup image -->
<link rel="apple-touch-startup-image" href="/icons/144.png">
<!-- enables or disables automatic detection of possible phone numbers -->
<meta name="format-detection" content="telephone=no">

<!-- Tile icon for Windows 8 (144x144 + tile color) -->
<meta name="msapplication-TileImage" content="/icons/144.png">
<meta name="msapplication-TileColor" content="#fff">
<meta name="msapplication-tap-highlight" content="no">
<meta name="msapplication-navbutton-color" content="#fff">
<meta name="msapplication-notification" content="frequency=30;polling-uri=http://notifications.buildmypinnedsite.com/?feed=https://bekti.net/feed.xml&amp;id=1;polling-uri2=http://notifications.buildmypinnedsite.com/?feed=https://bekti.net/feed.xml&amp;id=2;polling-uri3=http://notifications.buildmypinnedsite.com/?feed=https://bekti.net/feed.xml&amp;id=3;polling-uri4=http://notifications.buildmypinnedsite.com/?feed=https://bekti.net/feed.xml&amp;id=4;polling-uri5=http://notifications.buildmypinnedsite.com/?feed=https://bekti.net/feed.xml&amp;id=5; cycle=1"/>

<link href="/opensearch.xml" rel="search" title="<%= config.title %>" type="application/opensearchdescription+xml">
